{
	"name": "workout-ui",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "dotenvx run -f .env -- vite dev",
		"build": "dotenvx run -f .env -- vite build",
		"preview": "dotenvx run -f .env -- vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"supabase:gen": "supabase gen types --lang=typescript --project-id $SUPABASE_PROJECT_REF --schema public > src/lib/supabase/database.types.ts",
		"supabase:generate": "dotenvx run -f .env -- pnpm supabase:gen && pnpm format",
		"test:unit": "dotenvx run -f .env -- vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:ui": "vitest --ui",
		"test:e2e": "playwright test",
		"prepare": "husky"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.21.0",
		"@faker-js/faker": "^9.1.0",
		"@fontsource-variable/overpass": "^5.1.1",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/eslint": "^9.6.0",
		"@vite-pwa/sveltekit": "^0.6.6",
		"@vitest/ui": "^2.1.4",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"happy-dom": "^15.7.4",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"supabase": "^1.207.9",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"type-fest": "^4.26.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"@supabase/ssr": "^0.5.1",
		"@supabase/supabase-js": "^2.46.1"
	}
}
