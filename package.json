{
	"name": "workout-ui",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "dotenvx run -f .env -- vite dev",
		"build": "dotenvx run -f .env -- vite build",
		"preview": "dotenvx run -f .env -- vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"pwa:assets:generate": "pwa-assets-generator",
		"supabase:gen": "supabase gen types --lang=typescript --project-id $SUPABASE_PROJECT_REF --schema public > src/lib/supabase/database.types.ts",
		"supabase:generate": "dotenvx run -f .env -- pnpm supabase:gen && pnpm format",
		"test:unit": "dotenvx run -f .env -- vitest --run",
		"test": "npm run test:unit && npm run test:e2e",
		"test:ui": "vitest --ui",
		"test:e2e": "dotenvx run -f .env -- playwright test",
		"test:e2e:ui": "dotenvx run -f .env -- playwright test --ui",
		"prepare": "husky"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.34.0",
		"@faker-js/faker": "^9.4.0",
		"@fontsource-variable/overpass": "^5.1.2",
		"@inlang/paraglide-js": "1.11.8",
		"@inlang/paraglide-sveltekit": "0.15.5",
		"@material-symbols/svg-400": "^0.28.1",
		"@playwright/test": "^1.50.1",
		"@poppanator/sveltekit-svg": "^5.0.0",
		"@prgm/sveltekit-progress-bar": "^3.0.1",
		"@supabase/supabase-js": "^2.48.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-netlify": "^4.4.1",
		"@sveltejs/kit": "^2.17.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.6",
		"@types/eslint": "^9.6.1",
		"@types/google-one-tap": "^1.2.6",
		"@types/google.accounts": "^0.0.15",
		"@types/lodash": "^4.17.15",
		"@types/node": "^22.13.1",
		"@vite-pwa/assets-generator": "^0.2.6",
		"@vite-pwa/sveltekit": "^0.6.7",
		"@vitest/ui": "^3.0.5",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"happy-dom": "^16.8.1",
		"husky": "^9.1.7",
		"lodash": "^4.17.21",
		"material-symbols": "^0.28.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"sharp": "^0.33.5",
		"supabase": "^2.9.6",
		"svelte": "^5.19.7",
		"svelte-check": "^4.1.4",
		"svelte-french-toast": "^1.2.0",
		"sveltekit-superforms": "^2.23.1",
		"type-fest": "^4.33.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.23.0",
		"vite": "^6.0.11",
		"vite-plugin-pwa": "^0.21.1",
		"vitest": "^3.0.5",
		"workbox-window": "^7.3.0",
		"zod": "^3.24.1"
	},
	"dependencies": {
		"@supabase/ssr": "^0.5.2"
	},
	"lint-staged": {
		"*.{svelte,js,ts,css,md}": "prettier --write"
	}
}
